{
  "name": "AbuseRemoverBot",
  "description": "Telegram Anti-Abuse Bot | Delete abusive words and NSFW stickers automatically | Warn and Mute system",
  "repository": "https://github.com/BadshahAk/AbuseRemoverBot",
  "keywords": ["telegram", "bot", "antiabuse", "nsfw"],
  "env": {
    "MONGO_URI": {
      "description": "Your MongoDB URI",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Your Telegram Bot Token",
      "required": true
    },
    "API_ID": {
      "description": "Your Telegram API ID",
      "required": true
    },
    "API_HASH": {
      "description": "Your Telegram API Hash",
      "required": true
    },
    "OWNER_ID": {
      "description": "Owner User ID",
      "required": true
    },
    "LOGGER_ID": {
      "description": "Logger Channel ID",
      "required": true
    },
    "CRIME_CHANNEL": {
      "description": "Channel username where deleted abuse/sticker will be sent",
      "required": true
    }
  }
}
